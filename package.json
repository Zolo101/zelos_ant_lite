		"svelte-check": "^3.5.2",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^4.9.5",
		"vite": "^4.5.0"
	},
	"type": "module",
	"dependencies": {
		"add": "^2.0.6",
