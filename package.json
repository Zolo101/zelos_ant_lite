		"svelte-preprocess": "^5.0.1",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.2"
	},
	"type": "module",
	"dependencies": {
		"add": "^2.0.6",
